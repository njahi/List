generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Asset {
  id          String   @id @default(cuid())
  name        String
  value       Int
  profit      Int
  loss        Int
  year        Int
  dateCreated DateTime @default(now())
  dateUpdated DateTime @default(now())
}
